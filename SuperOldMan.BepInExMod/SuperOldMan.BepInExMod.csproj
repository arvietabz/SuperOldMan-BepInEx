		<Exec Command="if not exist &quot;$(MEGABONK_DIR)\plugins&quot; mkdir &quot;$(MEGABONK_DIR)\plugins&quot;" />
		<Exec Command="COPY &quot;$(TargetPath)&quot; &quot;$(MEGABONK_DIR)\plugins&quot;" />
